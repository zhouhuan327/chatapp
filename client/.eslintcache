/../store\";\nimport { getFriendMessage, getGroupMessage } from \"../../api\";\nimport moment from \"moment\";\n\nconst Conversation = () => {\n  // 自己的id\n  const { id } = useRecoilValue<UserInfo>(userInfoState);\n\n  const currentChat = useRecoilValue<RecentChat>(currentChatState);\n  // 消息列表\n  const [list, setList] = useState<any>([]);\n\n  const fetchFriendMessage = useCallback(async friendId => {\n    const res = await getFriendMessage({ friendId });\n    if (res?.code !== 200) return;\n    const data: Array<FriendMessage> = res.data;\n    setList(\n      data.map(item => {\n        // 判断这条消息是自己发的还是好友发的\n        const isMe = item.sender.id == id;\n        return renderBubbleElement(item, isMe);\n      }),\n    );\n  }, []);\n  const fetchGroupMessage = useCallback(async groupId => {\n    const res = await getGroupMessage({ groupId });\n    if (res?.code !== 200) return;\n    const data: Array<GroupMessage> = res.data;\n    setList(\n      data.map(item => {\n        // 判断这条消息是自己发的还是别人群员\n        const isMe = item.user.id == id;\n        return renderBubbleElement(item, isMe);\n      }),\n    );\n  }, []);\n  const renderBubbleElement = (item, isMe: boolean) => {\n    const time = moment(item.createTime).format(\"HH:mm\");\n    return isMe ? (\n      <MyChatBubble time={time}>{item.content}</MyChatBubble>\n    ) : (\n      <ChatBubble time={time}>{item.content}</ChatBubble>\n    );\n  };\n\n  useEffect(() => {\n    const { id, type } = currentChat;\n    if (type === \"friend\") {\n      fetchFriendMessage(id);\n    } else {\n      fetchGroupMessage(id);\n    }\n  }, [currentChat]);\n  const tBarAnimeProps = useSpring({\n    opacity: 1,\n    transform: \"translate3d(0px, 0px, 0px)\",\n    from: { opacity: 0, transform: \"translate3d(0px, -50px, 0px)\" },\n    delay: 500,\n  });\n\n  const convsAnimeProps = useSpring({\n    opacity: 1,\n    transform: \"translate3d(0px, 0px, 0px)\",\n    from: { opacity: 0, transform: \"translate3d(50px, 0px, 0px)\" },\n    delay: 800,\n  });\n\n  const ftAnimeProps = useSpring({\n    opacity: 1,\n    transform: \"translate3d(0px, 0px, 0px)\",\n    from: { opacity: 0, transform: \"translate3d(0px, 50px, 0px)\" },\n    delay: 950,\n  });\n  return (\n    <StyledConversation>\n      <TitleBar name=\"sean\" status=\"online\" animeProps={tBarAnimeProps} />\n      <Conversations style={convsAnimeProps}>{list}</Conversations>\n      <Footer animeProps={ftAnimeProps} />\n    </StyledConversation>\n  );\n};\n\nexport default memo(Conversation);\n","/Users/mac/code/chatapp-components/client/src/layout/Profile/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/layout/NavBar/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/layout/Conversation/style.ts",[],"/Users/mac/code/chatapp-components/client/src/layout/Profile/style.ts",[],"/Users/mac/code/chatapp-components/client/src/layout/NavBar/style.ts",[],"/Users/mac/code/chatapp-components/client/src/layout/Footer/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/Icon/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/FilterList/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/Switch/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/ParaGraph/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/utils/mixin.ts",[],"/Users/mac/code/chatapp-components/client/src/components/MessageCard/style.ts",[],"/Users/mac/code/chatapp-components/client/src/components/Avatar/style.ts",[],"/Users/mac/code/chatapp-components/client/src/components/MessageCard/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/Avatar/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/VoiceMessage/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/ChatBubble/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/layout/TitleBar/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/layout/Footer/style.t