t InputText, { InputTextProps } from \"./InputText/index\";\ninterface InputProps {\n  placeholder?: string;\n  prefix?: any;\n  suffix?: any;\n  [rest: string]: any;\n}\ninterface SearchProps {\n  placeholder?: string;\n}\ntype InputType = FC<InputProps> & {\n  Search: FC<SearchProps>;\n  Text: FC<InputTextProps>;\n};\nconst Input: InputType = ({\n  placeholder = \"请输入文字\",\n  prefix,\n  suffix,\n  onChange,\n}) => {\n  return (\n    <InputContainer>\n      {prefix && <Prefix>{prefix}</Prefix>}\n      <StyledInput\n        onChange={e => {\n          onChange && onChange(e.target.value);\n        }}\n        placeholder={placeholder}\n      ></StyledInput>\n      {suffix && <Suffx>{suffix}</Suffx>}\n    </InputContainer>\n  );\n};\n\nconst Search: FC<SearchProps> = ({ placeholder = \"请输入搜索内容...\" }) => {\n  const theme: any = useTheme();\n  return (\n    <Input\n      placeholder={placeholder}\n      prefix={\n        <Icon icon={SearchIcon} color={theme.gray3} width={18} height={18} />\n      }\n    />\n  );\n};\nInput.Search = Search;\nInput.Text = InputText;\n\nexport default Input;\n","/Users/mac/code/chatapp-components/client/src/components/Button/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/Popover/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/Select/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/Option/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/Button/style.ts",[],"/Users/mac/code/chatapp-components/client/src/components/Input/style.ts",[],"/Users/mac/code/chatapp-components/client/src/components/Popover/style.ts",[],"/Users/mac/code/chatapp-components/client/src/components/Select/style.ts",[],"/Users/mac/code/chatapp-components/client/src/components/Option/style.ts",[],"/Users/mac/code/chatapp-components/client/src/components/Input/InputText/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/Input/InputText/style.ts",[],"/Users/mac/code/chatapp-components/client/src/components/LabelContainer/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/store/socket.ts",["315"],"import { connect } from \"socket.io-client\";\nimport { atom, selector } from \"recoil\";\nimport { userInfoState } from \"./index\";\n\nexport const socketInstance = selector({\n  key: \"socketInstance\",\n  get: ({ get }) => {\n    const user = get(userInfoState);\n    const socket: SocketIOClient.Socket = connect(\n      `http://localhost:3000?userId=${user.id}`,\n    );\n    socket.on(\"connect\", () => {\n      console.log(\"socket连接成功\");\n    });\n    return socket;\n  },\n  dangerouslyAllowMutability: true,\n});\n","/Users/mac/code/chatapp-components/client/src/utils/lodable.tsx",[],"/Users/mac/code/chatapp-components/client/src/router/index.ts",[],"/Users/mac/code/chatapp-components/client/src/layout/Login/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/layout/Login/style.tsx",[],"/Users/mac/code/chatapp-components/client/src/utils/request.ts",[],"/Users/mac/code/chatapp-components/client/src/utils/auth.ts",[],"/Users/mac/code/chatapp-components/client/src/api/index.ts",[],"/Users/mac/code/chatapp-components/client/src/components/FriendCard/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/FriendCard/style.ts",[],"/Users/mac/code/chatapp-components/client/src/layout/FriendList/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/layout/FriendList/style.ts",[],"/Users/mac/code/chatapp-components/client/src/layout/GroupList/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/layout/GroupList/style.ts",[],"/Users/mac/code/chatapp-components/client/src/store/index.ts",[],"/Users/mac/code/chatapp-components/client/src/layout/RecentChatList/index.tsx",["316"],"/Users/mac/code/chatapp-components/client/src/layout/RecentChatList/style.ts",[],"/Users/mac/code/chatapp-components/client/src/hooks/useAnime.ts",[],"/Users/mac/code/chatapp-components/client/src/layout/ChatPanel/index.tsx",["317","318"],"import React, { memo, useCallback, useEffect, useRef, useState } from \"react\";\nimport StyledChatPanel, { Panels, MyChatBubble, NoticeBubble } from \"./style\";\nimport TitleBar from \"layout/TitleBar\";\nimport Footer from \"../Footer\";\nimport ChatBubble from \"components/ChatBubble\";\nimport { useRecoilValue } from \"recoil\";\nimport { currentChatState, userInfoState } from \"store\";\nimport { getFriendMessage, getGroupMessage } from \"api\";\nimport moment from \"moment\";\nimport { useChatAnime } from \"hooks/useAnime\";\nimport { socketInstance } from \"store/socket\";\n\nconst ChatPanel = () => {\n  // 动画参数\n  const { topBarAnime, msgAnime, footerAnime } = useChatAnime();\n  // socket实例\n  const socket = useRecoilValue(socketInstance);\n  // 消息列表的引用\n  const ref = useRef<HTMLDivElement>(null);\n  // 自己的id\n  const { id: userId } = useRecoilValue<UserInfo>(userInfoState);\n  // 当前的好友/群\n  const currentChat = useRecoilValue<RecentChat>(currentChatState as any);\n  // 消息列表\n  const [list, setList] = useState<Array<any>>([]);\n\n  const fetchList = useCallback(async currentChat => {\n    if (!currentChat.id) return;\n    if (currentChat.type === \"friend\") {\n      const res = await getFriendMessage({ friendId: currentChat.id });\n      res.code === 200 && setList(res.data);\n    } else {\n      const res = await getGroupMessage({ groupId: currentChat.id });\n      res.code === 200 && setList(res.data);\n    }\n  }, []);\n  // 记录已经建立过连接的好友/群 id\n  // 避免重复连接\n  const connectedFriend = useRef<Set<number>>(new Set());\n  const connectedGroup = useRef<Set<number>>(new Set());\n  useEffect(() => {\n    socket.on(\"friendChatConnect\", res => {\n      console.log(\"friend connect\", res);\n      addMessage({ notice: \"连接成功\" });\n      connectedFriend.current.add(res?.data?.receiverId);\n    });\n    socket.on(\"groupChatConnect\", res => {\n      console.log(\"group connect\", res);\n      addMessage({ notice: res.message });\n      connectedGroup.current.add(res?.data?.groupId);\n    });\n    // 消息实时更新\n    socket.on(\"friendChatMessage\", res => {\n      console.log(\"new friend message\", res);\n      addMessage(res.data);\n    });\n    socket.on(\"groupChatMessage\", res => {\n      console.log(\"new group message\", res);\n      addMessage(res.data);\n    });\n  }, []);\n  useEffect(() => {\n    if (!currentChat.id) return;\n    const { id, type } = currentChat;\n    if (type === \"friend\" && !connectedFriend.current.has(id)) {\n      socket.emit(\"friendChatConnect\", {\n        senderId: userId,\n        receiverId: id,\n      });\n    } else if (type === \"group\" && !connectedGroup.current.has(id)) {\n      socket.emit(\"groupChatConnect\", {\n        senderId: userId,\n        groupId: id,\n      });\n    }\n    // 切换聊天时,请求拿到聊天记录\n    fetchList(currentChat).then(() => scrollToBottom());\n  }, [currentChat]);\n\n  // 加入连接,发送消息时也要将轮动条拉到最低\n  useEffect(() => {\n    scrollToBottom();\n  }, [list]);\n  const addMessage = obj => {\n    setList(list => [...list, obj]);\n  };\n  const scrollToBottom = () => {\n    const el = ref.current;\n    if (el) {\n      el.scrollTop = el.scrollHeight;\n    }\n  };\n  const renderBubbleElement = list => {\n    return list.map(item => {\n      if (item.notice) {\n        return <NoticeBubble key={Math.random()}>{item.notice}</NoticeBubble>;\n      }\n      let isMe = false;\n      const time = moment(item.createTime).format(\"HH:mm\");\n      if (item.receiver?.id) {\n        // 好友消息\n        isMe = item.sender.id === userId;\n      } else {\n        // 群消息\n        isMe = item.user.id === userId;\n      }\n      return isMe ? (\n        <MyChatBubble key={item.id} time={time}>\n          {item.content}\n        </MyChatBubble>\n      ) : (\n        <ChatBubble key={item.id} time={time}>\n          {item.content}\n        </ChatBubble>\n      );\n    });\n  };\n  return (\n    <StyledChatPanel>\n      <TitleBar name=\"sean\" status=\"online\" animeProps={topBarAnime} />\n      <Panels ref={ref} style={msgAnime}>\n        {renderBubbleElement(list)}\n      </Panels>\n      <Footer\n        userId={userId}\n        currentChat={currentChat}\n        setList={setList}\n        animeProps={footerAnime}\n      />\n    </StyledChatPanel>\n  );\n};\n\nexport default memo(ChatPanel);\n","/Users/mac/code/chatapp-components/client/src/layout/ChatPanel/style.ts",[],"/Users/mac/code/chatapp-components/client/src/hooks/useSyncOnlineStatus.ts",["319","320"],{"ruleId":"321","replacedBy":"322"},{"ruleId":"323","replacedBy":"324"},{"ruleId":"325","severity":1,"message":"326","line":13,"column":10,"nodeType":"327","messageId":"328","endLine":13,"endColumn":24},{"ruleId":"329","severity":1,"message":"330","line":46,"column":6,"nodeType":"331","endLine":46,"endColumn":8,"suggestions":"332"},{"ruleId":"329","severity":1,"message":"333","line":61,"column":6,"nodeType":"331","endLine":61,"endColumn":19,"suggestions":"334"},{"ruleId":"321","replacedBy":"335"},{"ruleId":"323","replacedBy":"336"},{"ruleId":"321","replacedBy":"337"},{"ruleId":"323","replacedBy":"338"},{"ruleId":"325","severity":1,"message":"339","line":1,"column":21,"nodeType":"327","messageId":"328","endLine":1,"endColumn":29},{"ruleId":"325","severity":1,"message":"340","line":2,"column":10,"nodeType":"327","messageId":"328","endLine":2,"endColumn":14},{"ruleId":"329","severity":1,"message":"341","line":38,"column":6,"nodeType":"331","endLine":38,"endColumn":8,"suggestions":"342"},{"ruleId":"329","severity":1,"message":"343","line":61,"column":6,"nodeType":"331","endLine":61,"endColumn":8,"suggestions":"344"},{"ruleId":"329","severity":1,"message":"345","line":78,"column":6,"nodeType":"331","endLine":78,"endColumn":19,"suggestions":"346"},{"ruleId":"329","severity":1,"message":"347","line":19,"column":6,"nodeType":"331","endLine":19,"endColumn":12,"suggestions":"348"},{"ruleId":"329","severity":1,"message":"349","line":30,"column":6,"nodeType":"331","endLine":30,"endCol