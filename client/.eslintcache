mit(\"groupChatConnect\", {\n        senderId: userId,\n        groupId: id,\n      });\n    }\n  }, [currentChat]);\n  const onSubmit = () => {\n    const { id, type } = currentChat;\n    if (!id) return;\n    if (type === \"friend\") {\n      socket.emit(\"friendChatMessage\", {\n        senderId: userId,\n        receiverId: id,\n        content: message,\n        type: \"text\",\n      });\n    } else {\n      socket.emit(\"groupChatMessage\", {\n        senderId: userId,\n        groupId: id,\n        content: message,\n        type: \"text\",\n      });\n    }\n  };\n  return (\n    <StyledFooter style={{ ...animeProps }}>\n      <Input\n        placeholder=\"输入想要说的话\"\n        prefix={<Icon icon={ClipIcon} />}\n        onChange={v => {\n          setMessage(v);\n        }}\n        suffix={\n          <IconContainer>\n            <Popover offset={{ x: \"-25%\" }} content={<PopoverContent />}>\n              <Icon icon={SmileIcon} />\n            </Popover>\n            <Icon icon={MicrophoneIcon} />\n            <Button size=\"52px\" onClick={onSubmit}>\n              <Icon\n                color=\"white\"\n                icon={PlaneIcon}\n                style={{ transform: \"translateX(-2px)\" }}\n              />\n            </Button>\n          </IconContainer>\n        }\n      />\n    </StyledFooter>\n  );\n}\n\nexport default memo(Footer);\n","/Users/mac/code/chatapp-components/client/src/components/Icon/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/FilterList/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/Switch/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/components/ParaGraph/index.tsx",[],"/Users/mac/code/chatapp-components/client/src/utils/mixin.ts",[],"/Users/mac/code/chatapp-components/client/src/components/MessageCard/style.ts",[],"/Users/mac/code/chatapp-components/client/src/components/Avatar/style.ts",[],"/Users/mac/code/chatapp-components/client/src/components/MessageCard/i